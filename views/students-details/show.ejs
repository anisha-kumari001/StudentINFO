
<% layout("/layouts/boilerplate") %>
 <div class="row showpage mt-3 mb-3">
  <div class="col-8 offset-3">
    <h3><%= student.name %></h3>
    
    </div>
    <br><br><br>
    <div class="card cards col-5 offset-3 show card">
      <img src="<%=student.image%>" class="card-img-top show-img" alt="student_image">
    </div>
    <div class="card cards col-6 offset-3 show-card">
      
      <div class="card-body">
        <p class="card-text"
        ><br><br>
        REGISTRATION NO: <%= student.rollid %> <br>
         CG:  <%= student.cg%><br>
      SEMESTER:  <%= student.sem %> <br>
     
      <br>Marks in IWT :
      <%=student.courses.IWT %> <br>Marks in OS :
      <%= student.courses.OS %> <br>Marks in DBMS :
      <%= student.courses.DBMS %> <br>Marks in ML :
      <%= student.courses.ML %> <br>

      </p></div>
    </div>
    <br>
    <div class="btns">
    <a href="/students-details/<%= student._id %>/edit" class=" mt-3 btn btn-dark offset-3 edit-btn">Edit </a>
  
    
    
    
   
   